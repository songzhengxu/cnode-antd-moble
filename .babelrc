{
  "presets": [
    ["es2015", {"modules": false}],
    "stage-2",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["import", [{ "libraryName": "antd-mobile", "style": "css" }]],
  ],
  "env": {
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        },
        "test": {
          "presets": [
            "es2015",
            "stage-2",
            "react"
          ],
          "plugins": [
            ["module-resolver", {
              "alias": {
                  "~": ["./src"],
              }
            }]
          ]
        }
    }
}
